{
  "name": "redux-ready-wrapper",
  "version": "1.1.13",
  "description": "Ready wrapper middleware for Redux library",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run lint && npm run test && cross-env babel src --out-dir dist",
    "prepare": "npm run build",
    "lint": "eslint src __tests__",
    "test": "jest --coverage",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "roots": [
      "__tests__"
    ]
  },
  "keywords": [
    "redux",
    "middleware",
    "redux-middleware",
    "redux-ready-wrapper"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/borisding/redux-ready-wrapper.git"
  },
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^20.0.4",
    "nock": "^9.0.14",
    "node-fetch": "^1.7.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1"
  }
}
